type Query{
    customers: [Customer]
    customerById(id:ID): Customer
}

type Mutation {
    createCustomer(input:CreateCustomerInput):Customer
}

type Customer {
    id: ID
    name: String
    accountNumber: Int
}

input CreateCustomerInput {
    id: ID
    name: String
    accountNumber:Int
}



