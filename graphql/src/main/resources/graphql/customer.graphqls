type Query{
    customers: [Customer]
    customerById(id:ID): Customer
}

type Mutation {
    createCustomer(input:CreateCustomerInput):Customer
    deleteCustomer(id:ID):Boolean
}

type Customer {
    id: ID
    name: String
    accountNumber: Int
}

input CreateCustomerInput {
    id: ID
    name: String
    accountNumber:Int
}


